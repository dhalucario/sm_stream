{
  "name": "sm_stream_application",
  "version": "1.1.1",
  "description": "automatic melee stream application using slippi, with startgg integration",
  "main": "src/server.js",
  "bin": "src/server.js",
  "pkg": {
    "scripts": [
      "src/*.js"
    ],
    "assets": [
      "./src/**/*",
      "./static/*.ico",
      "./static/img/**/*"
    ],
    "targets": [
      "node20-win-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "start": "node src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-cleo/sm_stream_application.git"
  },
  "author": "cleo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-cleo/sm_stream_application/issues"
  },
  "homepage": "https://github.com/a-cleo/sm_stream_application#readme",
  "dependencies": {
    "@slippi/slippi-js": "^6.7.0",
    "@vinceau/slp-realtime": "^4.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "hbs": "^4.2.0",
    "obs-websocket-js": "^5.0.6",
    "rxjs": "^6.6.7",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.13.6",
    "chokidar": "^3.6.0"
  }
